{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
